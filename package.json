{
  "name": "vibethink-orchestrator",
  "version": "1.0.0",
  "description": "VThink 1.0 Enterprise SaaS Platform with AI Integration",
  "private": true,
  "scripts": {
    "dev": "cd apps/dashboard && npm run dev -- -p 3001",
    "dev:dashboard": "cd apps/dashboard && npm run dev -- -p 3001",
    "dev:admin": "cd apps/admin && npm run dev -- -p 3002",
    "dev:login": "cd apps/login && npm run dev -- -p 3003",
    "dev:helpdesk": "cd apps/helpdesk && npm run dev -- -p 3004",
    "dev:website": "cd apps/website && npm run dev -- -p 3005",
    "dev:test": "cd apps/dashboard && npm run dev -- -p 3099",
    "dev:test:reserve": "node dev-tools/utilities/port-manager.cjs test",
    "dev:all": "concurrently \"cd apps/dashboard && npm run dev -- -p 3001\" \"cd apps/admin && npm run dev -- -p 3002\" \"cd apps/login && npm run dev -- -p 3003\" \"cd apps/helpdesk && npm run dev -- -p 3004\"",
    "dev:status": "node dev-tools/utilities/port-manager.cjs status",
    "build": "npm run build:dashboard",
    "build:all": "npm run build:dashboard && cd apps/admin && npm run build && cd ../login && npm run build && cd ../helpdesk && npm run build && cd ../website && npm run build",
    "build:dashboard": "cd apps/dashboard && npx --no-install next build",
    "build:website": "cd apps/website && npx --no-install next build",
    "start": "cd apps/dashboard && npm run start",
    "start:website": "cd apps/website && npx --no-install next start",
    "lint": "cd apps/dashboard && npm run lint",
    "lint:all": "cd apps/dashboard && npm run lint && cd ../admin && npm run lint && cd ../login && npm run lint && cd ../helpdesk && npm run lint",
    "test": "cd apps/dashboard && npm run test",
    "test:all": "cd apps/dashboard && npm run test && cd ../admin && npm run test && cd ../login && npm run test && cd ../helpdesk && npm run test",
    "type-check": "cd apps/dashboard && npm run type-check",
    "validate": "npm run validate:quick && npm run validate:universal && npm run validate:routes",
    "validate:quick": "node dev-tools/validation/quick-validator.cjs",
    "validate:universal": "node dev-tools/validation/cross-app-validator.cjs && node dev-tools/validation/architecture-validator.cjs",
    "validate:full": "npm run validate:universal && node dev-tools/validation/shared-component-validator.cjs && node dev-tools/validation/dependency-validator.cjs",
    "validate:guard": "node dev-tools/validation/architecture-guard.cjs",
    "validate:security": "node dev-tools/validation/security-validator.cjs",
    "validate:arch": "node dev-tools/validation/architecture-validator.cjs",
    "validate:perf": "node dev-tools/validation/performance-validator.cjs",
    "validate:scripts": "node dev-tools/validation/script-placement-validator.cjs",
    "validate:dashboard": "node packages/cli/src/validation/dashboard-migration-guard.cjs",
    "validate:dashboard:global": "node packages/cli/src/validation/dashboard-migration-guard.cjs --global",
    "validate:routes": "node scripts/validate-dashboard-routes.js",
    "fix:deps": "node fix-npm-duplications.cjs",
    "pre-commit": "npm run validate:universal",
    "ai:commit": "npm run validate && git add . && git commit -m",
    "ai:recovery": "git stash && npm run validate:guard && git stash pop",
    "vhelp": "node dev-tools/utilities/vhelp-enhanced.cjs",
    "update:ui": "node scripts/update-shadcn.js",
    "update:ui:all": "node scripts/update-shadcn.js --all",
    "update:ui:list": "node scripts/update-shadcn.js --list",
    "vtk:validate": "npm run validate",
    "vtk:lint": "npm run lint",
    "vtk:test": "npm run test",
    "vtk:build": "npm run build",
    "vtk:dev": "npm run dev",
    "vhelp:help": "node dev-tools/utilities/vhelp-enhanced.cjs -h",
    "install:all": "npm install && cd apps/dashboard && npm install && cd ../admin && npm install && cd ../login && npm install && cd ../helpdesk && npm install && cd ../website && npm install",
    "clean": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"if (Test-Path 'node_modules') { Remove-Item -Recurse -Force 'node_modules' }; Get-ChildItem 'apps' -Directory | ForEach-Object { $nm = Join-Path $_.FullName 'node_modules'; if (Test-Path $nm) { Remove-Item -Recurse -Force $nm }; $next = Join-Path $_.FullName '.next'; if (Test-Path $next) { Remove-Item -Recurse -Force $next } }; Write-Host 'Cleanup completed!'\"",
    "clean:force": "node dev-tools/utilities/port-manager.cjs emergency && npm run clean",
    "setup": "npm run clean && npm run install:all",
    "port-check": "node dev-tools/utilities/port-manager.cjs status",
    "kill-ports": "node dev-tools/utilities/port-manager.cjs emergency",
    "quick-start": "npm run dev:status && npm run dev",
    "emergency": "node dev-tools/utilities/port-manager.cjs emergency && npm run validate:guard",
    "status": "npm run port-check && npm run validate:quick"
  },
  "keywords": [
    "vibethink",
    "orchestrator",
    "ai",
    "saas",
    "enterprise",
    "dashboard",
    "theme-customizer"
  ],
  "author": "VibeThink Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "workspaces": [
    "apps/*",
    "apps/*",
    "src/*",
    "packages/*"
  ],
  "dependencies": {
    "@fullcalendar/daygrid": "6.1.18",
    "@fullcalendar/interaction": "6.1.18",
    "@fullcalendar/list": "6.1.18",
    "@fullcalendar/react": "6.1.18",
    "@fullcalendar/timegrid": "6.1.18",
    "@hello-pangea/dnd": "18.0.1",
    "@hookform/resolvers": "5.2.1",
    "@radix-ui/react-accordion": "1.2.11",
    "@radix-ui/react-alert-dialog": "1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "2.1.5",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "1.0.7",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "1.0.4",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "1.1.3",
    "@radix-ui/react-scroll-area": "1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "1.1.5",
    "@radix-ui/react-toggle-group": "1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@remixicon/react": "^4.7.0",
    "@supabase/supabase-js": "2.53.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "0.5.16",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "0.2.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "framer-motion": "11.3.31",
    "lucide-react": "^0.522.0",
    "motion": "12.11.4",
    "next": "15.3.4",
    "next-themes": "0.4.6",
    "react": "18.3.1",
    "react-day-picker": "9.8.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.62.0",
    "react-is": "19.1.1",
    "recharts": "2.15.4",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "zod": "4.0.15",
    "zustand": "5.0.7"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.21",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-next": "15.3.4",
    "postcss": "8.4.31",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "5.9.2"
  },
  "ai-stability-rules": {
    "mandatory": true,
    "validation-required": true,
    "pre-commit-hook": true,
    "recovery-procedure": true
  }
}
