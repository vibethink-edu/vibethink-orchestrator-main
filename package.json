{
  "name": "vibethink-orchestrator",
  "version": "1.0.0",
  "description": "VibeThink Orchestrator - Plataforma SaaS Multi-tenant desarrollada con metodologÃ­a VThink 1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "bundui"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "bundui:install": "node bundui/scripts/install-bundui.js",
    "bundui:update": "node bundui/scripts/update-bundui.js",
    "bundui:build": "cd bundui && npm run build",
    "bundui:dev": "cd bundui && npm run dev",
    "bundui:test": "cd bundui && npm run test",
    "bundui:storybook": "cd bundui && npm run storybook",
    "bundui:build-storybook": "cd bundui && npm run build-storybook",
    "apps:dev": "lerna run dev --parallel",
    "apps:build": "lerna run build",
    "apps:test": "lerna run test",
    "apps:lint": "lerna run lint",
    "validate:vtk": "npm run type-check && npm run lint && npm run test",
    "validate:bundui": "npm run bundui:build && npm run bundui:test",
    "validate:all": "npm run validate:vtk && npm run validate:bundui",
    "setup:bundui": "npm run bundui:build && npm run bundui:install",
    "docs:bundui": "npm run bundui:storybook",
    "release:bundui": "cd bundui && npm version patch && npm run build && npm publish",
    "demo:bundui": "npm run bundui:storybook",
    "demo:apps": "npm run apps:dev"
  },
