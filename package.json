{
  "name": "@vibethink/orchestrator",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "lang-quality": "node scripts/lang-quality.js",
    "lang-quality:all": "node scripts/lang-quality.js --all",
    "validate:i18n": "node scripts/lang-quality.js",
    "i18n:validate-json": "node scripts/validate-i18n-json.js",
    "i18n:where": "node scripts/i18n-where.js",
    "i18n:smoke": "tsx scripts/i18n-smoke-test.ts"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google-cloud/translate": "^9.3.0",
    "dotenv": "^17.2.3",
    "tsx": "^4.21.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=10.2.4"
  },
  "packageManager": "npm@11.6.0",
  "dependencies": {
    "i18next": "^25.7.3",
    "react-i18next": "^16.5.0"
  }
}
