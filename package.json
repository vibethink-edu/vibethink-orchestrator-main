{
  "name": "vibethink-orchestrator",
  "version": "1.0.0",
  "description": "VThink 1.0 Enterprise SaaS Platform with AI Integration",
  "private": true,
  "scripts": {
    "dev": "npm run dev:dashboard",
    "dev:dashboard": "cd apps/dashboard && npm run dev",
    "dev:admin": "cd apps/admin && npm run dev",
    "dev:login": "cd apps/login && npm run dev",
    "dev:helpdesk": "cd apps/helpdesk && npm run dev",
    "dev:all": "concurrently \"npm run dev:dashboard\" \"npm run dev:admin\" \"npm run dev:login\" \"npm run dev:helpdesk\"",
    "build": "npm run build:all",
    "build:dashboard": "cd apps/dashboard && npm run build",
    "build:admin": "cd apps/admin && npm run build",
    "build:login": "cd apps/login && npm run build",
    "build:helpdesk": "cd apps/helpdesk && npm run build",
    "build:all": "npm run build:dashboard && npm run build:admin && npm run build:login && npm run build:helpdesk",
    "start": "npm run start:dashboard",
    "start:dashboard": "cd apps/dashboard && npm run start",
    "start:admin": "cd apps/admin && npm run start",
    "start:login": "cd apps/login && npm run start",
    "start:helpdesk": "cd apps/helpdesk && npm run start",
    "lint": "npm run lint:all",
    "lint:dashboard": "cd apps/dashboard && npm run lint",
    "lint:admin": "cd apps/admin && npm run lint",
    "lint:login": "cd apps/login && npm run lint",
    "lint:helpdesk": "cd apps/helpdesk && npm run lint",
    "lint:all": "npm run lint:dashboard && npm run lint:admin && npm run lint:login && npm run lint:helpdesk",
    "test": "npm run test:all",
    "test:dashboard": "cd apps/dashboard && npm run test",
    "test:admin": "cd apps/admin && npm run test",
    "test:login": "cd apps/login && npm run test",
    "test:helpdesk": "cd apps/helpdesk && npm run test",
    "test:all": "npm run test:dashboard && npm run test:admin && npm run test:login && npm run test:helpdesk",
    "validate:ecosystem": "node dev-tools/validation/cross-app-validator.cjs",
    "validate:cross-app-compatibility": "node dev-tools/validation/cross-app-validator.cjs",
    "validate:shared-component-impact": "node dev-tools/validation/shared-component-validator.cjs",
    "validate:external-update": "node dev-tools/validation/dependency-validator.cjs",
    "validate:security": "node dev-tools/validation/security-validator.cjs",
    "validate:performance": "node dev-tools/validation/performance-validator.cjs",
    "validate:duplication": "node dev-tools/validation/duplication-checker.cjs",
    "validate:integration": "node dev-tools/validation/integration-validator.cjs",
    "validate:ui-generic": "node dev-tools/validation/ui-generic-validator.cjs",
    "validate:architecture": "node dev-tools/validation/architecture-validator.cjs",
    "validate:guard": "node dev-tools/validation/architecture-guard.cjs",
    "validate:quick": "node dev-tools/validation/quick-validator.cjs",
    "validate:universal": "npm run validate:ecosystem && npm run validate:duplication && npm run validate:integration && npm run validate:ui-generic && npm run validate:architecture",
    "pre-commit": "npm run validate:universal",
    "ai:before-changes": "npm run validate:universal",
    "ai:after-changes": "npm run validate:universal && npm run dev",
    "ai:safe-commit": "npm run validate:universal && git add . && git commit -m",
    "ai:recovery": "git stash && npm run validate:universal && git stash pop",
    "install:all": "npm install && npm run install:apps",
    "install:apps": "cd apps/dashboard && npm install && cd ../admin && npm install && cd ../login && npm install && cd ../helpdesk && npm install",
    "clean": "npm run clean:all",
    "clean:all": "rm -rf node_modules && rm -rf apps/*/node_modules && rm -rf .next && rm -rf apps/*/.next",
    "setup": "npm run clean && npm run install:all"
  },
  "keywords": [
    "vibethink",
    "orchestrator",
    "ai",
    "saas",
    "enterprise",
    "dashboard",
    "theme-customizer"
  ],
  "author": "VibeThink Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "workspaces": [
    "apps/*",
    "src/*"
  ],
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/list": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@supabase/supabase-js": "^2.53.0",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.294.0",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "react": "^18",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18",
    "react-hook-form": "^7.62.0",
    "react-is": "^19.1.1",
    "recharts": "latest",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "15.3.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.2"
  },
  "ai-stability-rules": {
    "mandatory": true,
    "validation-required": true,
    "pre-commit-hook": true,
    "recovery-procedure": true
  }
}
