{
  "name": "@vibethink/orchestrator",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format:check": "turbo run format:check --continue",
    "format:write": "turbo run format:write --continue",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:unit": "turbo run test -- --testPathPattern='unit'",
    "test:integration": "turbo run test -- --testPathPattern='integration'",
    "test:contract": "turbo run test -- --testPathPattern='contract'",
    "test:api": "turbo run test -- --testPathPattern='api'",
    "clean": "turbo run clean",
    "lang-quality": "node scripts/lang-quality.js",
    "lang-quality:all": "node scripts/lang-quality.js --all",
    "validate:i18n": "node scripts/lang-quality.js",
    "i18n:validate": "node scripts/validate-i18n.js apps/dashboard/src/lib/i18n/translations",
    "i18n:validate-all": "node scripts/validate-i18n.js",
    "i18n:missing-keys": "echo 'TODO: Implement missing keys script'",
    "i18n:find-hardcoded": "echo 'TODO: Implement hardcoded strings finder'",
    "i18n:test-rtl": "echo 'TODO: Implement RTL testing script'",
    "i18n:coverage": "node scripts/lang-quality.js --all",
    "check-hardcoding": "node scripts/check-hardcoding.js",
    "validate:methodology-adoption": "node scripts/validate-methodology-adoption.mjs",
    "validate:fit-claims": "node scripts/validate-fit-claims.mjs",
    "validate:reports": "node scripts/validate-reports.js",
    "validate:root": "node scripts/validate-root-cleanliness.js",
    "formalize": "powershell -ExecutionPolicy Bypass -File ./scripts/git-formalize.ps1",
    "validate:pnpm": "pnpm install --frozen-lockfile --dry-run",
    "test:quick": "node scripts/tests/run-suite.js quick",
    "test:full": "node scripts/tests/run-suite.js full",
    "gate:banned": "node scripts/quality/gate-banned.mjs",
    "gate:quality": "pnpm run type-check && pnpm run lint && pnpm run test && pnpm run gate:banned",
    "validate:jsonb-schemas": "tsx scripts/validate-jsonb-schemas.ts",
    "validate:enums": "tsx scripts/validate-enums.ts",
    "validate:indexes": "tsx scripts/validate-indexes.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google-cloud/translate": "^9.3.0",
    "@types/pg": "^8.16.0",
    "@vitejs/plugin-react": "^5.1.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "dotenv": "^17.2.3",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "pg": "^8.16.3",
    "tsx": "^4.21.0",
    "turbo": "2.0.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "i18next": "^25.7.3",
    "react-i18next": "^16.5.0"
  }
}