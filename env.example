# VThink 1- Variables de Entorno
# ======================================

# Configuración de Base de Datos
# ------------------------------
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# API Keys para Integraciones
# ---------------------------
OPENAI_API_KEY=your_openai_api_key_here
FIRECRAWL_API_KEY=your_firecrawl_api_key_here
RESEND_API_KEY=your_resend_api_key_here
DART_AI_TOKEN=your_dart_ai_token_here
DART_AI_USER_ID=your_dart_ai_user_id_here

# Configuración de Email
# ----------------------
RESEND_FROM_EMAIL=noreply@vibethink.com
RESEND_DOMAIN=vibethink.com

# Configuración de Observabilidad
# -------------------------------
DATADOG_API_KEY=your_datadog_api_key_here
DATADOG_APP_KEY=your_datadog_app_key_here
DATADOG_SITE=datadoghq.com

# Configuración de Desarrollo
# ---------------------------
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3001
NEXT_PUBLIC_API_URL=http://localhost:3001

# Configuración de Seguridad
# --------------------------
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3001
# Configuración de CORS
# ---------------------
CORS_ORIGIN=http://localhost:3001
# Configuración de Logging
# ------------------------
LOG_LEVEL=info
ENABLE_DEBUG_LOGGING=true

# Configuración de Testing
# ------------------------
VITEST_ENV=test
PLAYWRIGHT_BASE_URL=http://localhost:3001
# Configuración de Bundui
# -----------------------
BUNDUI_API_KEY=your_bundui_api_key_here
BUNDUI_PROJECT_ID=vibethink-orchestrator

# Configuración de Monorepo
# -------------------------
LERNA_VERSION=independent 