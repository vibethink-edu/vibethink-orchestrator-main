# VibeThink Helpdesk Environment Variables
# Copy this file to .env.local and fill with your actual values

# App Configuration
NEXT_PUBLIC_APP_NAME="VibeThink Helpdesk"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# Database Configuration (Supabase)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Helpdesk Configuration
NEXT_PUBLIC_SUPPORT_EMAIL=support@vibethink.com
NEXT_PUBLIC_COMPANY_NAME="VibeThink"

# Development Configuration
NODE_ENV=development
NEXT_PUBLIC_PORT=3004

# Email Service (for tickets)
SMTP_HOST=your_smtp_host_here
SMTP_PORT=587
SMTP_USER=your_smtp_user_here
SMTP_PASS=your_smtp_password_here

# Security
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3004