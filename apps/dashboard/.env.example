# VibeThink Dashboard Environment Variables
# Copy this file to .env.local and fill with your actual values

# App Configuration
NEXT_PUBLIC_APP_NAME="VibeThink Dashboard"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# Database Configuration (Supabase)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Multi-tenant Configuration  
NEXT_PUBLIC_DEFAULT_COMPANY_ID=your_default_company_id_here

# Development Configuration
NODE_ENV=development
NEXT_PUBLIC_PORT=3001

# Optional: External Services
NEXT_PUBLIC_API_BASE_URL=http://localhost:3001/api

# Security
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3001