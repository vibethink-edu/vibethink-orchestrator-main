# Bundui Reference Environment Variables
# Copy this file to .env.local and fill with your actual values

# App Configuration  
NEXT_PUBLIC_APP_NAME="Bundui Reference"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# Development Configuration
NODE_ENV=development

# Reference Configuration
NEXT_PUBLIC_IS_REFERENCE_APP=true

# Optional: Demo data
NEXT_PUBLIC_ENABLE_DEMO_DATA=true

# Optional: Bundui specific
NEXT_PUBLIC_BUNDUI_VERSION="latest"